    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'rs-rocket-tflite'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=rs-rocket-tflite",
                    "--package=rs-rocket-tflite"
